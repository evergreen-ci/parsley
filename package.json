  "version": "0.0.0",
  "type": "commonjs",
  "scripts": {
    "build": "tsc && vite build",
    "build:local": "yarn build",
    "check-types": "tsc -p tsconfig.json --noEmit",
    "dev": "vite",
    "eslint:fix": "STRICT=1 eslint -c src/.eslintrc.js --fix 'src/**/*.ts?(x)'",
