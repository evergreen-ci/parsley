{
  "name": "parsley",
  "private": true,
  "version": "0.0.0",
  "type": "commonjs",
  "scripts": {
    "bootstrap-logkeeper": "./scripts/bootstrap-logkeeper.sh",
    "build": "tsc && . ./scripts/appVersion.sh && GIT_SHA=`git rev-parse HEAD` vite build",
    "build:local": "env-cmd -e local yarn build",
    "build:beta": "env-cmd -e beta yarn build",
    "build:staging": "env-cmd -e staging yarn build",
    "build:production": "env-cmd -e production yarn build",
    "check-types": "tsc -p tsconfig.json --noEmit",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "deploy:beta": "yarn build:beta && env-cmd -e beta yarn deploy:do-not-use",
    "deploy:staging": "yarn build:staging && env-cmd -e staging yarn deploy:do-not-use",
    "deploy:do-not-use": ". ./scripts/deploy.sh",
    "dev": "env-cmd -e devLocal vite",
    "staging": "env-cmd -e devStaging vite",
    "production": "env-cmd -e devProduction vite",
    "eslint:fix": "yarn eslint:strict --fix",
    "eslint:staged": "STRICT=1 eslint",
    "eslint:strict": "STRICT=1 eslint '*.{js,ts,tsx}' 'src/**/*.ts?(x)' 'cypress/**/*.ts'",
    "prepare": "husky install",
    "prettier": "prettier --write",
    "preview": "serve -s dist -p 4173",
    "snapshot": "jest storyshots.test.ts --watchAll=false",
    "start": "vite",
    "verify-backend": "./scripts/verify-backend.sh",
    "storybook": "start-storybook -p 6006",
    "storybook:build": " . ./scripts/appVersion.sh && env-cmd -e local build-storybook",
    "test": "jest --watchAll=false",
    "test:watch": "jest --watch --verbose",
    "upload-source-maps": "node ./scripts/upload-source-maps.js"
  },
  "dependencies": {
    "@bugsnag/js": "7.18.0",
    "@bugsnag/plugin-react": "7.18.0",
    "@emotion/react": "11.10.5",
    "@emotion/styled": "11.10.5",
    "@leafygreen-ui/badge": "7.0.0",
    "@leafygreen-ui/banner": "6.0.0",
    "@leafygreen-ui/button": "17.0.0",
    "@leafygreen-ui/card": "8.0.0",
    "@leafygreen-ui/confirmation-modal": "3.1.2",
    "@leafygreen-ui/emotion": "4.0.3",
    "@leafygreen-ui/icon-button": "14.0.0",
    "@leafygreen-ui/leafygreen-provider": "3.0.0",
    "@leafygreen-ui/modal": "11.0.0",
    "@leafygreen-ui/palette": "3.4.4",
    "@leafygreen-ui/segmented-control": "4.0.0",
